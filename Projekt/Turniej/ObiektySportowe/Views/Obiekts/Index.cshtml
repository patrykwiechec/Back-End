@model IEnumerable<ObiektySportowe.Models.Obiekt>

@{
    ViewData["Title"] = "Index";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Lokalizacja)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nazwa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IloscMiejsc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CenaBiletu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CenaBiletuVip)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Lokalizacja)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nazwa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IloscMiejsc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CenaBiletu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CenaBiletuVip)
            </td>
        </tr>
}
    </tbody>
</table>
