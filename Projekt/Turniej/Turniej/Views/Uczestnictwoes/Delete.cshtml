@model Turniej.Models.Uczestnictwo

@{
    ViewData["Title"] = "Usuń";
}
<script src="~/javascript.js"></script>
<h1>Delete</h1>

<h3>Jesteś pewny usunięcia tego uczestnictwa?</h3>
<div>
    <h4>Uczestnictwo</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdZawodnikaNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdZawodnikaNavigation.IdZawodnika)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdZawodowNavigation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdZawodowNavigation.IdZawodow)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdUczestnictwa" />
        <input type="submit" value="Usuń" class="btn btn-danger" /> |
        <a asp-action="Index">Wróć</a>
    </form>
</div>

<img class="sportimg" alt="Sport" src="~/sport.png" />
